{
  "name": "nanostyled",
  "version": "0.1.4",
  "description": "A < 1kb library for building styled React components, with CSS-in-JS-like APIs, without CSS in JS",
  "main": "index.js",
  "unpkg": "dist/nanostyled.umd.js",
  "scripts": {
    "bench": "NODE_ENV=production node test/benchmark.js",
    "precommit": "npm run build && npm run test && lint-staged",
    "test": "jasmine test/nanostyled.test.js",
    "build:umd": "rollup index.js --file dist/nanostyled.umd.js --format umd --name nanostyled",
    "build:cjs": "rollup index.js --file dist/nanostyled.cjs.js --format cjs",
    "build": "npm run build:umd && npm run build:cjs"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/chrisfrank/nanostyled"
  },
  "lint-staged": {
    "**/*.{js,jsx,jsxon,css,md}": [
      "prettier --single-quote --write --trailing-comma es5",
      "git add"
    ]
  },
  "keywords": [
    "react",
    "CSS",
    "css-in-js",
    "styled-components",
    "atomic CSS",
    "utility css",
    "functional CSS",
    "tachyons",
    "tailwind"
  ],
  "author": "Chris Frank",
  "license": "MIT",
  "devDependencies": {
    "benchmark": "^2.1.4",
    "browserify": "^16.2.3",
    "browserify-shim": "^3.8.14",
    "husky": "^0.14.3",
    "jasmine": "^3.2.0",
    "jest": "^23.6.0",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-test-renderer": "^16.5.1",
    "rollup": "^0.66.2",
    "styled-components": "^4.0.0-beta.9"
  },
  "peerDependencies": {
    "react": ">= 15"
  }
}
