{
  "name": "nanostyled",
  "version": "0.1.0",
  "description": "Build props-controlled, CSS-in-JS-like components using just functional CSS. Pairs well with tachyons, tailwindcss, etc.",
  "main": "dist/index.js",
  "scripts": {
    "bench": "NODE_ENV=production node test/benchmark.js",
    "build": "rollup -c",
    "precommit": "npm run build && lint-staged && npm run test",
    "start": "parcel example.html",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "github.com/chrisfrank/nanostyled"
  },
  "lint-staged": {
    "**/*.{js,jsx,jsxon,css,md}": [
      "prettier --single-quote --write --trailing-comma es5 **/*.js",
      "git add"
    ]
  },
  "keywords": [
    "tachyons",
    "css-in-js",
    "styled-components",
    "atomic CSS",
    "utility css",
    "functional CSS",
    "tailwind",
    "react",
    "CSS"
  ],
  "author": "Chris Frank",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "benchmark": "^2.1.4",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "lint-staged": "^7.2.2",
    "parcel-bundler": "^1.9.7",
    "prettier": "^1.14.2",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-test-renderer": "^16.5.1",
    "rollup": "^0.65.2",
    "rollup-plugin-babel": "^4.0.3",
    "styled-components": "^4.0.0-beta.4"
  },
  "peerDependencies": {
    "react": ">= 15"
  }
}
