{
  "name": "nanostyled",
  "version": "0.1.0",
  "description": "Build props-controlled, CSS-in-JS-like components using just functional CSS. Pairs well with tachyons, tailwindcss, etc.",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "bench": "NODE_ENV=production node test/benchmark.js",
    "precommit": "lint-staged && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "github.com/chrisfrank/nanostyled"
  },
  "lint-staged": {
    "**/*.{js,jsx,jsxon,css,md}": [
      "prettier --single-quote --write --trailing-comma es5 **/*.js",
      "git add"
    ]
  },
  "keywords": [
    "tachyons",
    "css-in-js",
    "styled-components",
    "atomic CSS",
    "utility css",
    "functional CSS",
    "tailwind",
    "react",
    "CSS"
  ],
  "author": "Chris Frank",
  "license": "MIT",
  "devDependencies": {
    "benchmark": "^2.1.4",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "jsdom": "^12.0.0",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "styled-components": "^3.4.6"
  },
  "peerDependencies": {
    "react": ">= 15"
  }
}
